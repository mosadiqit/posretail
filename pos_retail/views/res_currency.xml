<?xml version="1.0"?>
<odoo>
	<data>

		<record id="res_currency_multi_pricelist" model="ir.ui.view">
			<field name="name">res.currency.multi.pricelist</field>
			<field name="model">res.currency</field>
			<field name="inherit_id" ref="base.view_currency_form"/>
			<field name="arch" type="xml">
				<xpath expr = "//field[@name='rate']" position="after">
					<field name="converted_currency"/>
				</xpath>
			</field>
		</record>

	</data>
</odoo>
